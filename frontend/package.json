      "react-loadable",
      "react-redux/es",
      "react-router-dom/es",
      "react-router-redux/es",
      "redux/es/redux",
      "redux-immutable",
      "redux-localstorage",
      "redux-logger",
      "redux-saga/es",
      "react-vis/es",
      "reselect/es",
      "symbol-observable/es"
