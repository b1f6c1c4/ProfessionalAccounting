  "scripts": {
    "import-sort": "node ./internals/scripts/import-sort.js",
    "build": "cross-env API_URL='' node ./internals/scripts/build.js",
    "build:dev": "node ./internals/scripts/build.js --no-docker --profile",
    "build:dll": "node ./internals/scripts/build.js dll",
    "start": "cross-env NODE_ENV=development node server",
    "start:prod": "cross-env NODE_ENV=production node server",
    "clean": "rimraf stats.json ./build ./dll ./coverage",
    "generate": "plop --plopfile internals/generators/index.js",
    "lint": "eslint --ignore-path .gitignore .",
    "pretest": "yarn test:clean && yarn lint"
  },
  "private": true,
  "dllPlugin": {
    "path": "dll",
    "dlls": [
